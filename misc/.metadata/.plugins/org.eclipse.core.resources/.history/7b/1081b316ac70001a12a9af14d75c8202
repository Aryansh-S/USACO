#include <iostream>
#include <cstdio>
#include <set>
using namespace std;

typedef long long ll;
#define pb push_back
#define mp make_pair
#define f first
#define s second

#define problemname "cereal"

#define maxn 100000
#define maxm 100000
int n,m,data[2*maxn],ans[maxn]; set<int> s; //n cows, m cereals

int main(){
	ios_base::sync_with_stdio(0); cin.tie(0);
	//freopen(problemname ".in", "r", stdin); freopen(problemname ".out", "w", stdout);

	cin >> n >> m;
	for(int i=0; i<2*n; i++) cin >> data[2*n-i-1];
	for(int i=2*n-1; i>0; i-=2){
		s.insert(data[i]); s.insert(data[i-1]);
		ans[(i+1)/2-1]=s.size();
	}
	for(int i=0; i<n; i++) cout << ans[i] << "\n";
	return 0;
}
