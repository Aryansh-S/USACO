#include <iostream>
#include <cstdio>
#include <utility>
using namespace std;

typedef pair<int,int> pii;
typedef long long ll;
#define pb push_back
#define mp make_pair
#define f first
#define s second

#define problemname "cereal"
#define maxm 100000
#define maxn 100000

int n,m,cleft,cstate[maxm]; pii favs[maxn]; //min(#cows left, #diff favs left)

int main(){
	ios_base::sync_with_stdio(0); cin.tie(0);
	//freopen(problemname ".in", "r", stdin); freopen(problemname ".out", "w", stdout);

	cin >> n >> m;
	for(int i=0; i<n; i++){ //0 index cereals
		int a,b; cin >> a >> b; a--,b--; cstate[a]++,cstate[b]++;
		if(cstate[a]==1) cleft++;
		if(cstate[b]==1) cleft++;
		favs[i]=mp(a,b);
	}
	cout << min(cleft,n) << "\n";
	for(int i=1; i<n; i++){ //determine for cows i through n-1 only
		//remove cows[0] through cows[i]
	}

	return 0;
}
